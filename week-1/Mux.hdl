/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    Or(a=notSel, b=b, out=if1);
    Or(a=a, b=sel, out=if2);
    And(a=if1, b=if2, out=out);
}